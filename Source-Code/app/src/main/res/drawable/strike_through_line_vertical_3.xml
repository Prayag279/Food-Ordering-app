<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <rotate
            android:fromDegrees="0"
            android:toDegrees="0"
            android:pivotX="90%"
            android:pivotY="80%" >
            <shape
                android:shape="line"
                android:top="1dp" >
                <stroke
                    android:width="1dp" android:color="#b1b1b1"/>
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90"
            android:pivotX="60%"
            android:pivotY="50%" >
            <shape
                android:shape="line"
                android:top="1dp" >
                <stroke
                    android:width="1dp" android:color="#b1b1b1"/>
            </shape>
        </rotate>
    </item>

</layer-list>